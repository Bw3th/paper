#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_tiny-lr-fork@0.0.5@tiny-lr-fork/bin/tiny-lr" "$@"
  ret=$?
else 
  node  "$basedir/../../../_tiny-lr-fork@0.0.5@tiny-lr-fork/bin/tiny-lr" "$@"
  ret=$?
fi
exit $ret
